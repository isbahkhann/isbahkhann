public class work {
    public static void main(String[] args) {
        int a = 3;
        int b = 7;
        System.out.println("a+b=" + (a + b));
        System.out.println("a-b=" + (a - b));
        System.out.println("a*b =" + (a * b));
        System.out.println("a/b =" + (a / b));
        System.out.println("a%b =" + (a % b));
    }

}
